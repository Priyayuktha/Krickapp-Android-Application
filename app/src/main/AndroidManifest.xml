<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Krickapp">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".reg_account" />
        <activity android:name=".login" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".create_match" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".TeamDetailsActivity" android:exported="false"
            android:label="Team Details" />
        <activity android:name=".MatchesListActivity" />
        <activity android:name=".UserProfileActivity" android:exported="false" android:label="User Profile" />
        <activity android:name=".MoreActivity" android:exported="false" android:label="More" />
        <activity android:name=".AboutActivity" android:exported="false" android:label="About" />
        <activity android:name=".TermsActivity" android:exported="false" android:label="Terms &amp; Conditions" />
        <activity android:name=".tossresult" android:exported="false" android:label="Toss Result" />
        <activity android:name=".matchinfo" android:exported="false" android:label="Match Details" />
        <activity android:name=".scoredesk" android:exported="false" android:label="Score Desk" />
        <activity android:name=".matchresult" android:exported="false" android:label="Match Result" />
        <activity android:name=".select_player" android:exported="false" android:label="Select Players" />
    </application>

</manifest>